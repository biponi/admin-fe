  "build": {
    "appId": "com.prior.admin",
    "productName": "Prior Admin",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "public/electron.js",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.business"
